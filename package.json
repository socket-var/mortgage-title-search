{
  "name": "ask",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run server",
    "server": "node ./bin/www",
    "dev-server": "nodemon ./bin/www",
    "client": "cd client && npm start",
    "dev-start": "set NODE_ENV=development&set DEBUG=ask:server:*&concurrently --kill-others \"npm run dev-server\" \"npm run client\""
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^4.1.0",
    "cookie-parser": "~1.4.3",
    "crypto-js": "^3.1.9-1",
    "debug": "~2.6.9",
    "dotenv": "^7.0.0",
    "ethereumjs-tx": "^1.3.7",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "mongoose": "^5.4.20",
    "morgan": "~1.9.0",
    "truffle-hdwallet-provider": "^1.0.6",
    "web3": "^1.0.0-beta.51"
  },
  "devDependencies": {
    "nodemon": "^1.18.10"
  }
}
